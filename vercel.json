{
  "name": "job-search-app",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.png",
      "dest": "/favicon.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist",
  "installCommand": "npm install"
}