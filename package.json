{
  "name": "jobsearchapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export -p web && node -e \"require('fs').writeFileSync('dist/_redirects', '/_expo/* /_expo/:splat 200\\n/assets/* /assets/:splat 200\\n/favicon.ico /favicon.ico 200\\n/metadata.json /metadata.json 200\\n/* /index.html 200\\n')\"",
    "build:web": "expo export -p web && node -e \"require('fs').writeFileSync('dist/_redirects', '/_expo/* /_expo/:splat 200\\n/assets/* /assets/:splat 200\\n/favicon.ico /favicon.ico 200\\n/metadata.json /metadata.json 200\\n/* /index.html 200\\n')\"",
    "serve": "npx serve web-build -l 3000"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "axios": "^1.12.2",
    "expo": "~54.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-vector-icons": "^10.0.1",
    "nativewind": "^2.0.11",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.1",
    "tailwindcss": "^3.3.2"
  },
  "private": true
}
